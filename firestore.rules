service cloud.firestore {
  match /databases/{database}/documents {
    match /items/{item} {
      allow read: if request.auth.uid != null;
      allow write: if request.auth.uid != null;
    }
    match /associations/{association} {
      allow read: if request.auth.uid != null;
      allow write: if request.auth.uid != null;
    }
  }
}
